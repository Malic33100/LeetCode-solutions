class Solution(object):
    def twoSum(self, nums, target):
        
        pointer1 = 0

        while pointer1 < len(nums):
            pointer2 = pointer1 +1
            while pointer2 < len(nums):
                if nums[pointer1] + nums[pointer2] == target:
                    return pointer1, pointer2
                else:
                    pointer2 +=1
            pointer1 +=1