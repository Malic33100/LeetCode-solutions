class Solution(object):
    def searchInsert(self, nums, target):
        insert = len(nums)

        for i,value in  enumerate(nums,0):
            if nums[i] == target:
                return i
            else:
                i+=1
            
        
        for i, value in enumerate(nums,0):
            if nums[i] < target:
                i +=1
            elif nums[i] > target:
                return i
        return insert 
    
    # NEEDS REVISION
    #SOLUTION NEEDS TO RUN ON O LOG(N) TIME COMPLEXITY, BINARY SEARCH